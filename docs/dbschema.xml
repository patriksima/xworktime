    <View nm="xwt_dodavatel_dluh">
      <Sql>select d.*,sum(p.hodinyd * p.sazbad) AS dluh
from xwt_dodavatel as d
left join xwt_ukol as u on d.id = u.id_dodavatel 
left join xwt_podklady as p on u.id = p.id_ukol 
where u.status = 'dokonceny' and p.zaplacend = 0 
group by u.id_dodavatel;
</Sql>
    </View>
    <View nm="xwt_dodavatel_nefakturovano">
      <Sql>select d.*,sum(p.hodinyd * p.sazbad) AS nefakturovano
from xwt_dodavatel as d
left join xwt_ukol as u on d.id = u.id_dodavatel 
left join xwt_podklady as p on u.id = p.id_ukol 
where u.status = 'dokonceny' and p.fakturad = 0 and p.zaplacend = 0 
group by u.id_dodavatel;</Sql>
    </View>
    <View nm="xwt_zadavatel_dluh">
      <Sql>select z.*,sum(p.hodinyz * p.sazbaz) AS dluh 
from xwt_zadavatel as z 
left join xwt_ukol as u on z.id = u.id_zadavatel 
left join xwt_podklady as p on u.id = p.id_ukol 
where u.status = 'dokonceny' and p.fakturaz = 1 and p.zaplacenz = 0 
group by u.id_zadavatel;</Sql>
    </View>
    <View nm="xwt_zadavatel_naklady">
      <Sql>select z.*,sum(p.hodinyd * p.sazbad) AS naklady 
from xwt_zadavatel as z 
